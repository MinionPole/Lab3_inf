import re

def solve(s):
    if(s.count('/') != 2):
        return ("Не хайку. Должно быть 3 строки.")
    else:
        if( re.fullmatch("[^аоэеиыуёюяАОЭЕУИЫУЁЮЯ]*([аоэеиыуёюяАОЭЕУИЫУЁЮЯ][^аоэеиыуёюяАОЭЕУИЫУЁЮЯ]*){5}/[^аоэеиыуёюяАОЭЕУИЫУЁЮЯ]*([аоэеиыуёюяАОЭЕУИЫУЁЮЯ][^аоэеиыуёюяАОЭЕУИЫУЁЮЯ]*){7}/[^аоэеиыуёюяАОЭЕУИЫУЁЮЯ]*([аоэеиыуёюяАОЭЕУИЫУЁЮЯ][^аоэеиыуёюяАОЭЕУИЫУЁЮЯ]*){5}", s) == None):
            return "Не хайку."
        else:
            return "Хайку!"

s = input()
print(solve(s))
